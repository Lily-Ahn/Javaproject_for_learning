package com.test01;

public class Operator01 {
	
	public static void main(String[] args) {
		
		Operator01.test01();
		
		
	}
	
	// ! : 논리부정연산자
	// !true => false , !false => true
	public static void test01() {
		
		System.out.println("true의 부정 : " + !true);
		System.out.println("flase의 부정 : " + !false);
		
		
	}

}
