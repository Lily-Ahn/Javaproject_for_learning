package com.test01;

public class IfTest02 {
	
	//else if 
	//조건을 다중조건으로 줄 수 있다. 
	
	public void testElseIf() {
		
		int no1 = 30;
		int no2 = 20;
		
		if (no1>no2) {
			System.out.println(no1+"은 "+no2+"보다 큽니다.");	
		}
		
		else if (no1<no2) {
			System.out.println(no1+"은 "+no2+"보다 작습니다.");		
		}
		
		else {
			
			System.out.println(no1+"은 "+no2+"와 같습니다.");
		}
		
		
	}

}
